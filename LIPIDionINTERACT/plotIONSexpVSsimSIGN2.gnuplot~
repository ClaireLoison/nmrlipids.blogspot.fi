#set term postscript enhanced color  
set terminal postscript portrait enhanced color dashed lw 1 "Helvetica" 14 
set output "OrderParameterIONSexpVSsimSIGN2.eps"
set size 1.4, 0.85
set multiplot  layout 2,2
set xrange [-1:1001] 
set ytic 0.01
#set key above
set key at screen 0.6,screen 0.8 maxcols 2 maxrows 5
set key font ",13"


set yrange [-0.12:0.05]
set ylabel "{/Symbol b}  S_{CH}"
set origin 0,0.35
set size 0.65, 0.35
plot "./POPCnacl/betaORDPnaCONSexpSIGN.dat"  using 1:2 title 'DPPC+NaCl, ^2H NMR, T=332K' with linespoints  linetype 1 pt 9 lc rgb 'black', \
     "./POPCnacl/OrderParametersNACLcSIGN.dat" using 1:2 title 'POPC+NaCl, Berger, T=298K'  with linespoint linetype 1 pt 9, \
     "./POPCnacl/OrderParametersNACLcSIGN.dat" using 1:3 notitle with linespoints  linetype 1 pt 9, \
     "./POPCnacl/OrderParametersNACLcMACROGsign.dat" using 1:2 title 'POPC+NaCl, MacRog, T=310K' with linespoint linetype 1 pt 9 lc rgb 'blue', \
     "./POPCnacl/OrderParametersNACLcMACROGsign.dat" using 1:3 notitle with linespoints  linetype 1 pt 9 lc rgb 'blue', \
     "./POPCnacl/OrderParametersNACLcCHARMMsign.dat" using 1:2 title 'POPC+NaCl, CHARMM, T=303K' with linespoint linetype 1 pt 9 lc rgb 'green', \
     "./POPCnacl/OrderParametersNACLcCHARMMsign.dat" using 1:3 notitle with linespoints  linetype 1 pt 9 lc rgb 'green', \
     "./POPCnacl/OrderParametersNACLcORANGEsign.dat" using 1:2 title 'POPC+NaCl, Orange, T=298K' with linespoint linetype 1 pt 9 lc rgb 'orange', \
     "./POPCnacl/OrderParametersNACLcORANGEsign.dat" using 1:3 notitle with linespoints  linetype 1 pt 9 lc rgb 'orange'

set key at screen 1.3,screen 0.8 maxcols 2 maxrows 5
set yrange [-0.11:0.05]
#set ylabel "{/Symbol b}  S_{CH}"
set origin 0.7,0.35
set size 0.65, 0.35
plot "./POPCcacl/betaORDPcaCONSexpSIGN.dat"  using 1:2 title 'DPPC+CaCl_2, ^2H NMR, T=332K' with linespoints  linetype 1 pt 5 lc rgb 'black', \
     "./POPCcacl/OrderParametersCACLcSIGN.dat" using 1:2 title 'POPC+CaCl_2, Berger, T=298K' with linespoint linetype 1 pt 5 lc rgb 'red', \
     "./POPCcacl/OrderParametersCACLcSIGN.dat" using 1:3 notitle with linespoints  linetype 1 pt 5 lc rgb 'red', \
     "./POPCcacl/OrderParametersCACLcORANGEsign.dat" using 1:2 title 'POPC+CaCl_2, Orange, T=298K' with linespoint linetype 1 pt 5 lc rgb 'orange', \
     "./POPCcacl/OrderParametersCACLcORANGEsign.dat" using 1:3 notitle with linespoints  linetype 1 pt 5 lc rgb 'orange'

set yrange [-0.015:0.11]
set ylabel "{/Symbol a}  S_{CH}"
set xlabel "[NaCl] (mM)"
set origin 0,0
set size 0.65, 0.35
plot "./POPCnacl/alphaORDPnaCONSexp.dat"  using 1:2 notitle with linespoints  linetype 1 pt 9 lc rgb 'black', \
     "./POPCnacl/OrderParametersNACLcSIGN.dat" using 1:4 notitle  with linespoint linetype 1 pt 9, \
     "./POPCnacl/OrderParametersNACLcSIGN.dat" using 1:5 notitle with linespoints  linetype 1 pt 9, \
     "./POPCnacl/OrderParametersNACLcMACROGsign.dat" using 1:4 notitle  with linespoint linetype 1 pt 9 lc rgb 'blue' , \
     "./POPCnacl/OrderParametersNACLcMACROGsign.dat" using 1:5 notitle with linespoints  linetype 1 pt 9 lc rgb 'blue', \
     "./POPCnacl/OrderParametersNACLcORANGEsign.dat" using 1:4 notitle with linespoint linetype 1 pt 9 lc rgb 'orange', \
     "./POPCnacl/OrderParametersNACLcORANGEsign.dat" using 1:5 notitle with linespoints  linetype 1 pt 9 lc rgb 'orange', \
     "./POPCnacl/OrderParametersNACLcCHARMMsign.dat" using 1:4 notitle with linespoint linetype 1 pt 9 lc rgb 'green', \
     "./POPCnacl/OrderParametersNACLcCHARMMsign.dat" using 1:5 notitle with linespoints  linetype 1 pt 9 lc rgb 'green'



set yrange [-0.015:0.11]
#set ylabel "{/Symbol a}  S_{CH}"
set xlabel "[CaCl] (mM)"
set origin 0.7,0
set size 0.65, 0.35
plot "./POPCcacl/alphaORDPcaCONSexp.dat"  using 1:2 notitle with linespoints  linetype 1 pt 5 lc rgb 'black', \
     "./POPCcacl/OrderParametersCACLcSIGN.dat" using 1:4 notitle with linespoint linetype 1 pt 5 lc rgb 'red', \
     "./POPCcacl/OrderParametersCACLcSIGN.dat" using 1:5 notitle with linespoints  linetype 1 pt 5 lc rgb 'red', \
     "./POPCcacl/OrderParametersCACLcORANGEsign.dat" using 1:4 notitle with linespoint linetype 1 pt 5 lc rgb 'orange', \
     "./POPCcacl/OrderParametersCACLcORANGEsign.dat" using 1:5 notitle with linespoints  linetype 1 pt 5 lc rgb 'orange'


unset multiplot

